{
    "diagnostic-languageserver.filetypes": {
        "sh": "shellcheck"
    },
    "diagnostic-languageserver.formatFiletypes": {
        "bash": "shfmt-bash",
        "sh": "shfmt-posix"
    },
    "diagnostic-languageserver.formatters": {
        "shfmt-posix": {
            "command": "shfmt",
            "args": [
                "-i",
                "4",
                "-bn",
                "-ci",
                "-sr",
                "-ln",
                "posix"
            ]
        },
        "shfmt-bash": {
            "command": "shfmt",
            "args": [
                "-i",
                "4",
                "-bn",
                "-ci",
                "-sr",
                "-ln",
                "bash"
            ]
        }
    },
    "list.normalMappings": {
        "<C-n>": "do:next",
        "<C-p>": "do:previous"
    },
    "latex.build.args": [
        "-pdf",
        "-interaction=nonstopmode",
        "-synctex=1",
        "-pv"
    ],
    "latex.build.onSave": true,
    "latex.lint.onChange": true,
    "coc.preferences.extensionUpdateCheck": "daily",
    "coc.preferences.currentFunctionSymbolAutoUpdate": true,
    "suggest.enablePreview": true,
    "diagnostic.warningSign": "!!",
    "diagnostic.errorSign": "!!",
    "diagnostic.hintSign": "i",
    "diagnostic.infoSign": "i",
    "suggest.triggerAfterInsertEnter": true,
    "codeLens.enable": true,
    "list.nextKeymap": "<C-n>",
    "list.previousKeymap": "<C-p>",
    "list.source.diagnostics.pathFormat": "short",
    "suggest.completionItemKindLabels": {
        "keyword": "\uf1de",
        "variable": "\ue79b",
        "value": "\uf89f",
        "operator": "\u03a8",
        "constructor": "\uf0ad",
        "function": "\u0192",
        "reference": "\ufa46",
        "constant": "\uf8fe",
        "method": "\uf09a",
        "struct": "\ufb44",
        "class": "\uf0e8",
        "interface": "\uf417",
        "text": "\ue612",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "module": "\uf40d",
        "color": "\ue22b",
        "property": "\ue624",
        "field": "\uf9be",
        "unit": "\uf475",
        "event": "\ufacd",
        "file": "\uf723",
        "folder": "\uf114",
        "snippet": "\ue60b",
        "typeParameter": "\uf728",
        "default": "\uf29c"
    }
}
